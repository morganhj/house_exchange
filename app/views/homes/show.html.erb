<div class="container">
      <section class="house">
        <div class="house-header">
          <h1 id="house-name"><%= @home.name %></h1>
          <p><svg
            class="house-score-star"
            width="1em"
            height="1em"
            viewBox="0 0 16 16"
            class="bi bi-star-fill"
            fill="gold"
            xmlns="http://www.w3.org/2000/svg"
          >
            <path
              d="M3.612 15.443c-.386.198-.824-.149-.746-.592l.83-4.73L.173 6.765c-.329-.314-.158-.888.283-.95l4.898-.696L7.538.792c.197-.39.73-.39.927 0l2.184 4.327 4.898.696c.441.062.612.636.283.95l-3.523 3.356.83 4.73c.078.443-.36.79-.746.592L8 13.187l-4.389 2.256z"
            />
          </svg> 4.5 | <span id="house-address"><%= @home.address %></span></p>
        </div>
        <div class="row">
          <% if @home.photos.first.nil? %>
              <div class="col-6 d-flex justify-content-center">
                <%= cl_image_tag "no_photo", height: 510, width: 510, crop: :fill, class: "home-photo dropdown-toggle" %>
              </div>
              <div class="col-6">
                <div class="small-images-container">
                  <% 4.times do %>
                  <%= cl_image_tag "no_photo", height: 250, width: 250, crop: :fill, class: "home-photo dropdown-toggle" %>
                  <% end %>
                </div>
              </div>
          <% else %>
              <div class="col-6 d-flex justify-content-center">
              <%= cl_image_tag @home.photos.first.key, height: 510, width: 510, crop: :fill, class: "home-photo dropdown-toggle" %>
              </div>
              <div class="col-6">
                <div class="small-images-container">
                  <% @home.photos.each do |photo| %>
                  <%= cl_image_tag photo.key, height: 250, width: 250, crop: :fill, class: "home-photo dropdown-toggle" %>
                  <% end %>
                </div>
              </div>
          <% end %>
        </div>
      </section>
      <div class="row">
        <!--#region Description-->
        <div class="col-6 house-description mt-3">
          <h2>Description</h2>
          <p>
            <%= @home.description %>
          </p>
        </div>
        <!--#endregion Description-->
        <!--#region calendar-->
        <div class="col-6 calendar-container">
          <%= render "calendar_form" %>
          
        </div>
        <!-- #endregion calendar -->
      </div>

